<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <TableView fx:id="questionTable" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="10.0">
        <columns>
            <TableColumn text="ID" fx:id="idColumn" prefWidth="50" />
            <TableColumn text="Content" fx:id="contentColumn" prefWidth="200" />
            <TableColumn text="Date Published" fx:id="dateColumn" prefWidth="100" />
            <TableColumn text="Category" fx:id="categoryColumn" prefWidth="100" />
            <TableColumn text="Likes" fx:id="likesColumn" prefWidth="50" />
            <TableColumn text="Dislikes" fx:id="dislikesColumn" prefWidth="50" />
        </columns>
    </TableView>
</AnchorPane>
